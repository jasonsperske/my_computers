<div class='row'>
  <div class="col-md-9">
    <div class='row'>
      <div class="col">
        <h1>Jason Sperske</h1>
        <p>I write a lot of software</p>
      </div>
    </div>
    <div class='row'>
      <div class="col">
        <h2>Projects I have started</h2>
      </div>
    </div>
    <div class='row'>
    <% projectsStarted.forEach((project) => { %>
      <div class="col-lg-6">
        <div class="card">
          <div class="card-header"><%=project.name%> (<%=project.yearsActive%>)</div>
          <div class="card-body">
            <div class="text-center">
              <img class="img-fluid" src="<%=project.image%>" alt=""/>
            </div>
            <p class="card-text"><%-project.description%></p>
            <a href='project/<%=project.id%>' class='btn btn-primary'>learn more</a>
          </div>
        <% if (project.status) { %>
          <div class="card-footer text-muted"><strong>Status:</strong> <%-project.status%></div>
        <% } %>
        </div>
      </div>
    <% }) %>
    </div>

    <div class='row'>
      <div class="col">
        <h2>Things I have done for no good reason</h2>
      </div>
    </div>
    <div class='row'>
    <% projectsPersonal.forEach((project) => { %>
      <div class="col-lg-6">
        <div class="card">
          <div class="card-header"><%=project.name%></div>
          <div class="card-body">
            <p class="card-text"><%-project.description%></p>
            <a href='<%=project.href%>' class='btn btn-primary'>learn more</a>
          </div>
        </div>
      </div>
    <% }) %>
    </div>
  </div>
  <div class='col'>
    <h4>Places you can find me</h4>
  <% bio.links.forEach(link => { %>
    <a class="btn btn-block btn-info" href="<%=link.href%>"><i class="fa fa-<%=link['fa-icon']%>"></i> <%=link.label%></a>
  <% }) %>
  </div>
</div>
